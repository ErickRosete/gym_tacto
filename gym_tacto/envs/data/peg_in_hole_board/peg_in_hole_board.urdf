<?xml version="1.0"?>
<robot name="board">
    <material name="brown">
        <color rgba="0.87 0.81 0.76 1.0"/>
    </material>

    <link concave="yes" name="board">
        <inertial>
            <origin xyz="-0.00026 0.0 0.11809" rpy="0 0 0"/>
            <mass value="3.70082"/>
